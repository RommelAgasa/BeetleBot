{
  "expo": {
    "name": "RoboCar Controller",
    "slug": "robocar-controller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/workshopLogo.png",
    "scheme": "robocarcontroller",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/workshopLogo.png",
        "backgroundColor": "#45556c"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.scintilla.robocarcontroller"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/workshopLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#45556c"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "10fcdb49-72f3-48be-909c-f6f435e68ae9"
      }
    }
  }
}
